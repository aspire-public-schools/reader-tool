
<div class="container">
  <div class="content">
  <h1>Welcome to the Aspire Reader Tool </h1>

    <div class="hero-unit">
      <%= form_for @reader, url: sessions_path do |f| %>
        <%= f.text_field :email, placeholder: "Enter your Aspire Email" %>
        <%= f.submit "Login", class: "btn btn-default btn-sm" %>
      <% end %>
    </div>
  </div>
</div>


