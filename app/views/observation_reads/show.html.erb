<div class="container-fluid">
  <div class="row">
    <div class="sidebar fixed">
      <%= render partial: "sidebar" %>
    </div>
  </div>

  <div class="content">
    <div class="content right">

      <div class='row'>
        <h2>Observation Read <%= @observation_read.id %></h2>
      </div>

      <div class='row'>
        <div class="main-dashboard">
          <div class="col-md-6">
            <div class="dashboard-holder">
              <%= render partial: "domain_percentages" if @domain_percentages && @get_section_scores %>
            </div>
          </div>

          <div class="col-md-6">
            <div class="document-live-form-holder">
              <%= render partial: "document-live-form" if @observation_read %>
            </div>
          </div>
        </div>
      </div>

      <div id="evidence-form-holder"></div>
    </div>
  </div>
</div>
