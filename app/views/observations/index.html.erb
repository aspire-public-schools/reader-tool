<nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
  <div class="navbar-inner">
    <div class="container-fluid">
      <a class="navbar-brand" href="#">Aspire Reader</a>
      <ul class="nav navbar-right">
        <li><p class="navbar-text navbar-left">Welcome, <%= @reader.first_name %><p></li>
        <li><%= button_to "logout", logout_path, method: 'delete', class: "btn btn-default navbar-btn navbar-right" %></li>
      </ul>
    </div>
  </div>
</nav>

<div class="container-fluid">


<%= @observation_read %>

<% if @domains %>
  <div class="row">
    <div class="sidebar">
      <div class="sidebar fixed">
        <%= render partial: "sidebar" %>
      </div>
    </div>
  </div>
  <% if @indicators %>
      <div class="content">
      <div class="content right">
      Observation Numbers:
      <% @observation_reads.each do |observation| %>
        |<%= link_to "#{observation.observation_group_id}", observation_path(observation) %>
      <% end %>

        <% evidence_scores.each do |banana| %>
          <p><%= banana.description %></p>
        <% end %>

        <%= form_for @reader do |f| %>
          <%= check_box_tag(:description) %>
          <%= label_tag(:pet_dog, "I own a dog") %>
        <% end %>
      </div>
    </div>
  </div>
  <% end %>
<% end %>
</div>









