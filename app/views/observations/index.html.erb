<div class="container-fluid">
  <div class="row">
    <div class="sidebar">
      <div class="sidebar fixed">
        <%= render partial: "sidebar" %>
      </div>
    </div>
  </div>

    <div class="content">
      <div class="content right">

      <div class="table-responsive">
        <table class="table table-condensed table-bordered">
          <tr>
            <td></td>
            <td>Domain 1</td>
            <td>Domain 2</td>
            <td>Domain 3</td>
            <td>Domain 4</td>
          </tr>
          <tr>
            <td>Quality</td>
            <td><%= @domain_percentages[0].quality_average.to_f.round(2) %></td>
            <td><%= @domain_percentages[1].quality_average.to_f.round(2) %></td>
            <td><%= @domain_percentages[2].quality_average.to_f.round(2) %></td>
            <td><%= @domain_percentages[3].quality_average.to_f.round(2) %></td>
          </tr>
          <tr>
            <td>Alignment</td>
            <td><%= @domain_percentages[0].alignment_average.to_f.round(2) %></td>
            <td><%= @domain_percentages[1].alignment_average.to_f.round(2) %></td>
            <td><%= @domain_percentages[2].alignment_average.to_f.round(2) %></td>
            <td><%= @domain_percentages[3].alignment_average.to_f.round(2) %></td>
          </tr>
        </table>
      </div>

        <%= render partial: "observation_reads" %>

      <div class="evidence-form-holder">
      </div>

      <div class="evidence-form-update">
      </div>

      </div>
    </div>
</div>











