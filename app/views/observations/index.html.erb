<nav class="navbar navbar-inverse navbar-static-top" role="navigation">
  <div class="navbar-inner">
    <div class="container-fluid">
      <a class="navbar-brand" href="#">Aspire Reader</a>
      <ul class="nav navbar-right">
        <li><p class="navbar-text navbar-left">Welcome, <%= @reader.first_name %><p></li>
        <li><%= button_to "logout", logout_path, method: 'delete', class: "btn btn-default navbar-btn navbar-right" %></li>
      </ul>
    </div>
  </div>
</nav>


  <div class="panel-group" id="accordion">

      <div class="panel panel-default">
      <%= @domain %>
        <% @domain.each do |domain_obj| %>
          <div class="panel-heading">
            <h4 class="panel-title">
                <%= domain_obj %>
              <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                <%= domain_obj.description %>
              </a>
            </h4>
          </div>
          <div id="collapse<%= int_to_words(domain_obj.id) %>" class="panel-collapse collapse in">
            <div class="panel-body">
              <ul>
                <div class="button">
                  <% domain_obj.indicators.each do |indicator| %>
                    <li><%= indicator.description %></li>
                  <% end %>
                </div>
              </ul>
            </div>
          </div>
        <% end %>
      </div>
  </div>
