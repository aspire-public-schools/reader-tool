<dl>
  <% @domain.each do |domain_obj| %>
     <dt>
        <h5>
          <%= domain_obj.description %>
          <span class="caret"></span>
        </h5>
      </dt>
          <% domain_obj.indicators.each do |indicator| %>
            <dd class="indicators">
              <a href="#">
                <strong><%=indicator.code %></strong>:<%= indicator.description %>
              </a>
            </dd>
          <% end %>
    <% end %>
</dl>
