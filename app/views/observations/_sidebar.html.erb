<dl>
  <% unless @observation_read.is_a?(Array) %>
  <% @domains.each do |domain_score| %>
     <dt>
        <h4>
          <%= domain_score.domain.description %>
          <span class="caret"></span>
        </h4>
      </dt>
          <% domain_score.indicator_scores.sort! { |a,b| a.indicator.code <=> b.indicator.code} %>
          <% domain_score.indicator_scores.each do |indicator_score| %>
              <dd class="indicators">
                <span>
                <%= link_to "#{indicator_score.indicator.code}: #{indicator_score.indicator.description}", observation_domain_indicator_evidences_path(@observation_read, domain_score.domain, indicator_score), class: "#{indicator_score.id}", id: "indicator", :remote => true %>
                </span>
              </dd>
          <% end %>
        <% end %>
    <% end %>
</dl>