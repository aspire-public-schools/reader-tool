<% if @domain_percentages %>
  <table class="table table-condensed table-bordered" id="domain-table">
      <tr>
        <td></td>
        <% @domain_percentages.each do |domain_percentage| %>
          <td> Domain <%=domain_percentage.number%> </td>
        <% end %>
      </tr>
      <tr>
        <td>Quality</td>
        <% @domain_percentages.each do |domain_percentage| %>
          <td><%= number_with_precision(domain_percentage.quality_average.to_f*100, precision: 1) %> %</td>
        <% end %>
      </tr>
      <tr>
        <td>Alignment</td>
        <% @domain_percentages.each do |domain_percentage| %>
          <td><%= domain_percentage.alignment_average.to_f.round(3)*100 %> %</td>
        <% end %>
      </tr>
  </table>
<% end %>